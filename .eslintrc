{
  "extends": "rackt",
  "rules": {
    "valid-jsdoc": 2,
    "no-var": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2,
    "react/jsx-uses-vars": 1,
    "prefer-const": 2,
    "indent": [ 2, 4, {"SwitchCase": 1} ]
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}